<h1>New Property</h1>

<%= form_with(model: @property, url: properties_path, local: true) do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>

  <div>
    <%= form.label :city %>
    <%= form.text_field :city %>
  </div>